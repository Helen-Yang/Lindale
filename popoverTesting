<script>
$(document).ready(function(){
    $('[data-toggle="popover"]').popover();   
});

$("#doge").hover(function () {
    $(this).popover({
        title: "Doge",
        placement:"bottom",

        content: "Level: 1 <br />Songs: 3 <br />",
        // content: <a href="Main Homepage.html"> Log Out</a>
        html: true
    }).popover('show');
}, function () {
    $(this).popover('hide');
});


</script>

